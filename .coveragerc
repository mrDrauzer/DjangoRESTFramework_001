[run]
branch = True
source = 
    lms
    users
    config
omit =
    */migrations/*
    */tests.py
    manage.py
    */__init__.py
    */wsgi.py
    */asgi.py
    */settings.py

[report]
show_missing = True
skip_covered = True
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.: 

[html]
directory = htmlcov